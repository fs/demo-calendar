if @success
  page.call 'showNotice', 'Successfully updated event.'
  page.select(".event.#{@event.id}").invoke('remove')
  @event.split.each do |e|
    page << "if($('#{e.starts_at.strftime("%m-%d-%Y")}')){$('#{e.starts_at.strftime("%m-%d-%Y")}').insert('#{escape_javascript(render(:partial => 'event', :object => e))}')};"
  end
  page << "showEvent('#{@event.id}')"
else
  page.replace_html 'event_form', :partial => 'form'
end