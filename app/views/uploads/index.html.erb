<% title "Uploads" %>

<table>
  <tr>
    <th>Attachment File Name</th>
    <th>Attachment Content Type</th>
    <th>Attachment File Size</th>
  </tr>
  <% for upload in @uploads %>
    <tr>
      <td><%=h upload.attachment_file_name %></td>
      <td><%=h upload.attachment_content_type %></td>
      <td><%=h upload.attachment_file_size %></td>
      <td><%= link_to "Show", upload %></td>
      <td><%= link_to "Edit", edit_upload_path(upload) %></td>
      <td><%= link_to "Destroy", upload, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Upload", new_upload_path %></p>
